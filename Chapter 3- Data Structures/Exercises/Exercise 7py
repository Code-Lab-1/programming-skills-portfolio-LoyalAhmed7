locations =['Venice' ,'Canada', 'Switzerland', 'Japan' , 'Malaysia']
print(sorted(locations))
print(locations)
print(locations)
print(locations)
locations.sort()
print(sorted(locations, reverse=True))
print(locations)
locations.reverse()
print(locations)
locations.sort(reverse = True)
print(locations)
locations.reverse()
print(locations)
